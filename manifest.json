{
    "manifest_version": 2,
    "name": "Manga Arrows",
    "version": "1.4",

    "description": "adds Arrow functionality for prev/next Chapter on some Manga Readers",

    "content_scripts": [
        {
            "matches": ["*://manganelo.com/chapter/*", "*://www.webtoons.com/en/*", "*://*/comics/*", "*://asurascans.com/*"],
            "js": ["mangaArrows.js"]
        }
    ]
}
